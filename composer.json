{
  "name": "kyoya/php-job-runner",
  "description": "This is a framework to run jobs (maybe cron-jobs). Each job is separated into three tasks: fetching data from source, process fetched data, put into target format.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Krenz",
      "email": "info@kyoya.de"
    }
  ],
  "require": {
    "symfony/console": "~2.6",
    "symfony/process": "~2.6",
    "symfony/dependency-injection": "~2.6",
    "symfony/config": "~2.6",
    "symfony/yaml": "~2.6",
    "symfony/serializer": "~2.6",
    "symfony/finder": "~2.6",
    "symfony/filesystem": "~2.6",
    "psr/log":"~1.0",
    "symfony/event-dispatcher":"~2.6",
    "monolog/monolog":"~1.13",
    "kyoya-de/process":"dev-master"
  },
  "autoload":{
    "classmap":["src/"]
  },
  "repositories": [
    {
      "type":"composer",
      "url":"https://github.com/kyoya-de/Process.git"
    }
    {
      "type": "package",
      "package": {
        "name": "",
        "version": "2.6",
        "dist": {
          "url": "https://github.com/kyoya-de/Process/archive/2.6.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:kyoya-de/Process.git",
          "type": "git",
          "reference": "refs/2.6"
        }
      }
    }  ]
}
